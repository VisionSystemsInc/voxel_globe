
PGENGINE=${ROAM_DIR}
PGPORT=${POSTGRESQL_PORT}
PGDATA=${POSTGRESQL_DATABASE}
PGLOG=${POSTGRESQL_LOG}
# Log file for pg_upgrade
PGUPLOG=${POSTGRESQL_LOG}.upgrade

pidfile=${PID_DIR}/postgresql.pid
lockfile=${LOCALSTATEDIR}/lock/subsys/postgresql

PG_USER=${POSTGRESQL_USER}
PG_GROUP=${DAEMON_GROUP}

PGOPTS=

export POSTGIS_ENABLE_OUTDB_RASTERS=1
export POSTGIS_GDAL_ENABLED_DRIVERS=ENABLE_ALL
export POSTGIS_GDAL_ENABLED_DRIVERS="GTiff PNG JPEG GIF XYZ"

