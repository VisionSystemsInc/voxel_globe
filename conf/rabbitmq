PATH=/sbin:/usr/sbin:/bin:/usr/bin
NAME=rabbitmq-server
#DAEMON=${NPR_ROAM_DIR}/rabbitmq-server
DAEMON=${NPR_LIBDIR}/rabbitmq/sbin/rabbitmq-server
CONTROL=${NPR_LIBDIR}/rabbitmq/sbin/rabbitmqctl
INIT_LOG_DIR=${NPR_RABBITMQ_LOG_DIR}
PID_FILE=${NPR_RABBITMQ_PID_DIR}/$NAME.pid
LOCK_FILE=${NPR_RABBITMQ_LOCK_DIR}/$NAME

export RABBITMQ_LOG_BASE=${NPR_RABBITMQ_LOG_DIR}
export RABBITMQ_MNESIA_BASE=${NPR_RABBITMQ_MNESIA_BASE}
export ERLANG_HOME=${NPR_RABBITMQ_ERLANG_HOME}

