#!/bin/bash

source $(dirname ${BASH_SOURCE[0]})/../vip.bsh

dropdb ${VIP_POSTGRESQL_DOCK_CREDENTIALS} ${VIP_POSTGRESQL_DATABASE_NAME} ;:

createdb ${VIP_POSTGRESQL_DOCK_CREDENTIALS} --encoding ${VIP_POSTGRESQL_ENCODING} ${VIP_POSTGRESQL_DATABASE_NAME}

psql ${VIP_POSTGRESQL_DOCK_CREDENTIALS} -d ${VIP_POSTGRESQL_DATABASE_NAME} < "$1"
